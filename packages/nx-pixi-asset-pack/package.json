{
  "name": "@plastic-ant/nx-pixi-asset-pack",
  "version": "0.1.2",
  "license": "MIT",
  "description": "An Nx (Project Crystal) plugin for developing with Pixi AssetPack",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "types": "dist/index.cjs.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.cjs",
      "types": "./dist/index.esm.d.ts",
      "import": "./dist/index.esm.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/plastic-ant/packages.git",
    "directory": "packages/nx-pixi-asset-pack"
  },
  "homepage": "https://github.com/plastic-ant/packages/tree/main/packages/nx-pixi-asset-pack#readme",
  "bugs": {
    "url": "https://github.com/plastic-ant/packages/issues"
  },
  "keywords": [
    "nx",
    "crystal",
    "plugin",
    "assetpack",
    "pixi"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "executors.json",
    "generators.json",
    "!**/*.tsbuildinfo",
    "!**/*.map"
  ],
  "publishConfig": {
    "access": "public",
    "scope": "@plastic-ant"
  },
  "author": {
    "name": "pas-mike",
    "url": "https://github.com/pas-mike"
  },
  "funding": {
    "type": "buymeacoffee",
    "url": "https://buymeacoffee.com/plasticant"
  },
  "generators": "./generators.json",
  "executors": "./executors.json",
  "devDependencies": {
    "nx": "^20.8.1",
    "@nx/devkit": "^20.8.1",
    "@assetpack/core": "^1.4.0",
    "json-schema-to-typescript": "^15.0.3",
    "typescript-json-schema": "0.65.1"
  },
  "dependencies": {},
  "peerDependencies": {
    "nx": "^20.8.1",
    "@nx/devkit": "^20.8.1",
    "@assetpack/core": "^1.4.0"
  }
}
