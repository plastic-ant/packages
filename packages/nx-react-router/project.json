{
  "name": "nx-react-router",
  "targets": {
    "version": {},
    "github": {},
    "npm": {},
    "prebuild": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/nx-react-router/src",
        "commands": ["json2ts -i executors/build/schema.json -o executors/build/schema.d.ts --format"]
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "assets": ["packages/nx-react-router/src/executors/build/schema.json"]
      }
    }
  }
}
