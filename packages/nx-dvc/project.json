{
  "name": "nx-dvc",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/nx-dvc/src",
  "projectType": "library",
  "tags": ["#nx-plugin"],
  "targets": {
    "version": {},
    "github": {},
    "npm": {},
    "lint": {},
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "assets": [
          {
            "input": "packages/nx-dvc",
            "glob": "**/*.json",
            "ignore": ["**/tsconfig*.json", "project.json", ".eslintrc.json"],
            "output": "/"
          },
          {
            "input": "packages/nx-dvc",
            "glob": "**/*.js",
            "ignore": ["**/*.config.{ts,js,cjs,mjs,mts}"],
            "output": "/"
          },
          {
            "input": "packages/nx-dvc",
            "glob": "**/*.d.ts",
            "output": "/"
          },
          {
            "input": "",
            "glob": "README.md",
            "output": "/"
          },
          {
            "input": "",
            "glob": "LICENSE",
            "output": "/"
          }
        ]
      }
    }
  }
}
