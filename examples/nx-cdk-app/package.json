{
  "name": "nx-cdk-app",
  "version": "0.1.0",
  "private": true,
  "bin": "bin/nx-cdk-app.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk"
  },
  "nx": {
    "targets": {
      "synth": {
        "dependsOn": [
          "cdk-synth"
        ],
        "command": "#### Done! (inferred) ####"
      },
      "cdk-synth": {
        "options": {
          "args": [
            "--debug",
            "--strict"
          ]
        }
      },
      "postTarget": {
        "command": "#### Done! ####"
      },
      "cdk-bootstrap-executor": {
        "executor": "@plastic-ant/nx-cdk:bootstrap",
        "options": {
          "account": "123456789",
          "region": "us-east-2",
          "postTargets": [
            "nx-cdktf-app:postTarget"
          ]
        }
      },
      "cdk-synth-executor": {
        "executor": "@plastic-ant/nx-cdk:synth",
        "options": {
          "debug": true,
          "postTargets": [
            "nx-cdktf-app:postTarget"
          ]
        }
      },
      "cdk-deploy-executor": {
        "executor": "@plastic-ant/nx-cdk:deploy",
        "options": {
          "postTargets": [
            "nx-cdktf-app:postTarget"
          ]
        }
      }
    }
  },
  "devDependencies": {
    "aws-cdk": "^2.177.0",
    "ts-node": "10.9.2",
    "typescript": "5.5.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.177.0",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  }
}
