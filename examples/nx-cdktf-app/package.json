{
  "name": "nx-cdktf-app",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "MPL-2.0",
  "private": true,
  "nx": {
    "targets": {
      "cdktf-get": {
        "options": {
          "args": [
            "--language=typescript"
          ]
        }
      },
      "postTarget": {
        "command": "#### Done! ####"
      },
      "cdktf-deploy-executor": {
        "executor": "@plastic-ant/nx-cdktf:deploy",
        "options": {
          "postTargets": [
            "nx-cdktf-app:postTarget"
          ]
        }
      },
      "cdktf-synth-executor": {
        "executor": "@plastic-ant/nx-cdktf:synth",
        "options": {
          "postTargets": [
            "nx-cdktf-app:postTarget"
          ]
        }
      },
      "cdktf-get-executor": {
        "executor": "@plastic-ant/nx-cdktf:get",
        "options": {
          "postTargets": [
            "nx-cdktf-app:postTarget"
          ]
        }
      }
    }
  },
  "engines": {
    "node": ">=18.0"
  },
  "dependencies": {
    "cdktf": "^0.21.0",
    "constructs": "^10.4.2"
  },
  "devDependencies": {
    "cdktf-cli": "^0.21.0",
    "jest": "^29.4.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3"
  }
}
