{
  "name": "nx-strapi-app",
  "private": true,
  "version": "0.1.0",
  "description": "A Strapi application",
  "nx": {
    "targets": {
      "lint": {
        "executor": "nx:noop"
      },
      "strapi-develop": {
        "options": {
          "args": [
            "--open"
          ]
        }
      },
      "start-local-db": {
        "command": "docker run --name postgresql-container -p 5432:5432 -e POSTGRES_DB=$DATABASE_NAME -e POSTGRES_PASSWORD=$DATABASE_PASSWORD -d postgres"
      }
    }
  },
  "dependencies": {
    "@strapi/plugin-cloud": "5.12.6",
    "@strapi/plugin-users-permissions": "5.12.6",
    "@strapi/strapi": "5.12.6",
    "pg": "8.15.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.13",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^18",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "uuid": "cbe60171-b3f4-45c2-aa95-6d99db2cf827"
  }
}
